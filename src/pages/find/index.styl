.find{
  background: #f8f8f8
  .swiper-box{
    width: 100%
    height: 450px
  }
  .img-item{
    height:100%
    width: 100%
    .img{
      width: 100%
      height: 100%
    }
  }
  &_classtify-nav{
    padding: 10px 30px;
    background: #fff
    text{
      font-size: 32px
      line-height: 60px
    }
    .ul_one{
      display: flex
      text-align: center
    }
    .ul_two{
      // margin-top: 10px
      display: flex
      text-align: center
    }
    .one_item{
      width: 25%
      height: calc((100vw - 92px)/4)
      background: #ccc
      margin: 4px
    }
    .item{
      position relative
      image{
        width: 100%
        height: calc((100vw - 92px)/4)
      }
      text{
        display: block
        position: absolute
        top: 0
        width: 100%
        background: rgba(0,0,0,.5)
        color #fff
        line-height: calc((100vw - 92px)/4)
      }
    }
    
    .last-item{
      width: calc(25% - 4px)
      // background: #fff
    }
    .two_item{
      width: 50%
      // padding-bottom: 20%
      background: #ccc;
      margin: 4px
    }
  }
  &_content-list{
    padding: 20px 20px
    .item{
      padding: 30px
      margin-bottom: 30px
      background: #fff
      border-radius: 12px
      &_header{
        display: flex
        font-size: 36px
        text{
          margin-left: 30px
        }
      }
      &_content{
        padding-top: 40px
        text{
          font-size: 34px
        }
      }
      &_img-box{
        display: flex
        flex-wrap: wrap
        margin-top: 20px
        image{
          max-width: 210px
          max-height: 220px
          padding: 5px
        }
      }
      &_date-handle{
        font-size: 32px
        color: #ccc
        display: flex
        justify-content: space-between
        padding: 40px 0
        .icon{
          display: block
          view{
            margin-left: 30px
          }
          .liked{
            color: #ff5a5f
          }
        }
      }
    }
  }
}